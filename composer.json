{
    "name": "manriel/moonshine-settings",
    "description": "Settings package for Moonshine admin ui",
    "keywords": ["laravel", "admin", "dashboard", "moonshine", "settings"],
    "type": "library",
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "moonshine/moonshine": "^2.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Shershnev",
            "email": "readytoban@gmail.com",
            "homepage": "https://github.com/Manriel"
        }
    ],
    "autoload": {
        "psr-4": {
            "MoonShineSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoonShineSettings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShineSettings\\MoonShineSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
